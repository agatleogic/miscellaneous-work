datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id            Int      @id @default(autoincrement())
  title         String
  price         Int
  category      String
  isAvalaiable  Boolean @default(false)
  description   String?
  createdAt     DateTime  @default(now())
}